{
    "tabWidth": 4,
    "printWidth": 80,
    "semi": true,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "useTabs": false,
    "trailingComma": "none",
    "jsxBracketSameLine": false,
    "singleQuote": false,
    "jsxSingleQuote": false,
    "importOrder": ["^@cthit/react-digit-components", "api/(.*)$", "common/(.*)$","^[./]"],
    "importOrderSeparation": true,
    "experimentalBabelParserPluginsList": [
        "jsx",
        "flow"
    ]
}
